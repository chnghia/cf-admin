  <%
  function active(page, data) {
    if(page === data) return 'active';
    if(~url.indexOf('/' + page)) return 'active';
    return '';
  }
  %>
    <header class="">
      <div class="navbar navbar-inverse">
      <div class="navbar-inner">
        <div class="container-fluid">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <a class="brand" href="#" name="top">cf-admin</a>
          <div class="nav-collapse collapse">
            <ul class="nav">
              <li class="<%= active('home', url)%>"><a href="/home"><i class="icon-home icon-white"></i> Home</a></li>
              <li class="<%= active('dashboard', url)%>"><a href="/dashboard"><i class="icon-file icon-white"></i> Dashboard</a></li>
              <!--li><a href="#"><i class="icon-envelope icon-white"></i> Messages</a></li>
              <li><a href="#"><i class="icon-signal icon-white"></i> Stats</a></li>
              <li><a href="#"><i class="icon-lock icon-white"></i> Permissions</a></li-->
            </ul>
            
              <% if (!user) { %>
              <div class="btn-group pull-right">
              <a class="btn" href="/login"><i class="icon-user"></i> Sign In</a>
              </div>
              <% } else { %>
              <div class="btn-group pull-right">
              <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
                <i class="icon-user"></i> <%= user.username %> <span class="caret"></span>
              </a>
              <ul class="dropdown-menu">
                <li><a href="/settings"><i class="icon-wrench"></i> Settings</a></li>
                <li class="divider"></li>
                <li><a href="/logout"><i class="icon-share"></i> Logout</a></li>
              </ul>
              </div>
              <% } %>
            
          </div> <!--/.nav-collapse -->
        </div> <!--/.container-fluid -->
      </div> <!--/.navbar-inner -->
    </div>
    </header>
